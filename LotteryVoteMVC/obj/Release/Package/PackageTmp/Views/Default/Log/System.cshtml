@using LotteryVoteMVC.Models
@using LotteryVoteMVC.Utility
@model PagedList<LotteryVoteMVC.Models.SystemError>
@{
    ViewBag.Title = "System";
}
<link href="@Url.Content("~/Content/Pager/pager.css")" rel="stylesheet" type="text/css" />
<h2>
    System</h2>
<table>
    <tr>
        <th>
            ErrorDate
        </th>
        <th>
            ErrorMessage
        </th>
        <th>
            PageUrl
        </th>
        <th>
            Remarks
        </th>
        <th>
            IP
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ErrorDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ErrorMessage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PageUrl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Remarks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IP)
            </td>
            <td>
                @Html.ActionLink("Details", "DetailSystem", new { id = item.Id })
            </td>
        </tr>
    }
</table>
@Html.Pager(Model)
